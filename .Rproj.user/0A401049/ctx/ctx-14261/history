add_reused_chart_elements <- function(g) {
g +
scale_x_discrete(labels = c("M", "T", "W", "T", "F", "S", "S")) +
facet_wrap(~Publisher) +
ylim(0, 65) +
guides(fill = FALSE) +
labs(title = "Crossword Completion Times",
x = "Day of the Week",
y = "Completion Time (Minutes)")
}
